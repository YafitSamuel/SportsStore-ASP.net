



@using SportsStore.Models;

<link href="@Url.Content("~/Content/StyleSheet2.css")" rel="stylesheet" type="text/css" />

<table style="width:100%">

    <tr>

        <th> Gender</th>
        <th> Company</th>
        <th> Model</th>
        <th> Price</th>
        <th> Amount</th>
        <th> IsShort</th>
        <th> IsDreyfit</th>
        <th> Image</th>
    </tr>

    @foreach (Clothing item in @ViewBag.Clothings)

    {
        <tr>
            <td> @item.Gender</td>
            <td> @item.Company</td>
            <td> @item.Model</td>
            <td> @item.Price</td>
            <td> @item.Amount</td>
            <td> @item.IsShort</td>
            <td> @item.IsDreyfit</td>
            <td>  <img src=" @item.Image" width="50px" height="50px" /> </td>
        </tr>

    }

</table>
